<title>{{model.title}}</title>
<meta property="og:title" content={{model.title}}>
<meta name="twitter:title" content={{model.title}}>

<meta name="msapplication-navbutton-color" content="#000000">

<meta property="og:site_name" content="sarahtherad.com">
<meta property="og:type" content="website">
{{#with model.description as |description|}}
  <meta name="description" content={{model.description}}>
  <meta property="og:description" content={{model.description}}>
  <meta name="twitter:description" content={{model.description}}>
{{/with}}
<meta name="twitter:card" content="summary">
{{#with model.url as |url|}}
  <meta property="og:url" content={{model.url}}>
  <meta name="twitter:url" content={{model.url}}>
{{/with}}

{{#with model.prev as |prev|}}
  <link rel="next" href={{model.next}}>
{{/with}}
{{#with model.next as |next|}}
  <link rel="next" href={{model.next}}>
{{/with}}

{{#with model.jsonld as |jsonld|}}
  <script type="application/ld+json">{{json-stringify jsonld}}</script>
{{/with}}
